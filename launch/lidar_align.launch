<launch>

  <node pkg="lidar_align" name="lidar_align" type="lidar_align">
    <param name="input_bag_path" value="/home/z/Datasets/out.bag" />
    <param name="use_n_scans" value="3000" />

    <param name="min_point_curvature" value="0.2" />
    <param name="min_point_distance" value="2" />
    <param name="max_point_distance" value="50" />
    <param name="keep_points_ratio" value="0.5" />
    <param name="minium_angular_velocity" value="0.0" />

    <param name="knn_batch_size" value="5000" />
    <param name="knn_k" value="1" />
    <param name="knn_max_dist" value="0.2" />
    <param name="joint_self_compare" value="false" />
  </node>

</launch>